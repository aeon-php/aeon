{
    "name": "ocelot/calendar",
    "type": "library",
    "description": "PHP type safe, immutable calendar library",
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpstan/phpstan": "^0.12.25",
        "vimeo/psalm": "^3.11",
        "phpstan/phpstan-phpunit": "^0.12.8",
        "phpstan/phpstan-webmozart-assert": "^0.12.4",
        "psalm/plugin-phpunit": "^0.10.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "contact@norbert.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ocelot\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ocelot\\Calendar\\Tests\\": "tests/Ocelot/Calendar/Tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "webmozart/assert": "^1.8"
    },
    "scripts": {
        "test": "phpunit",
        "static:analyze": [
            "psalm",
            "phpstan analyze -c phpstan.neon"
        ]
    }
}
